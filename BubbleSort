# Task3
package Qsort;

/**
 * Created by Natalia on 04.04.2015.
 */
public class BubbleSort implements Sorting {
    @Override
    public void sort(int[] array) {
        for (int i = 0; i < array.length-1; i++){
            for (int j = i+1; j < array.length; j++){
                if (array[i] < array[j]) {
                    int t = array[i];
                    array[i] = array[j];
                    array[j] = t;
                }
            }
        }
    }
    }
